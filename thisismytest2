  if ((mintnumber >= 1499) && (mintnumber < 1499)) {
        mintnumber = mintnumber + 7;
        writeCookie('minted', mintnumber, 30);
        document.getElementById("mintnumber").innerHTML = mintnumber;
    }
    if (mintnumber >= 1499) clearInterval(interval);
}
